#cube demo
ldm
xt=0
yt=0
zt=0
xr=20
yr=30
zr=45
xs=20
ys=20
zs=20
#Front Face
drl .5,.5,.5 to -.5,.5,.5
drl -.5,.5,.5 to -.5,-.5,.5
drl -.5,-.5,.5 to .5,-.5,.5
drl .5,-.5,.5 to .5,.5,.5
#Right Face
drl .5,.5,.5 to .5,.5,-.5
drl .5,-.5,.5 to .5,-.5,-.5
#Left Face
drl -.5,.5,.5 to -.5,.5,-.5
drl -.5,-.5,.5 to -.5,-.5,-.5
#Back Face
drl .5,.5,-.5 to -.5,.5,-.5
drl -.5,.5,-.5 to -.5,-.5,-.5
drl -.5,-.5,-.5 to .5,-.5,-.5
drl .5,-.5,-.5 to .5,.5,-.5
